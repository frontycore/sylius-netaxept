{
    "name": "fronty/sylius-netaxept",
    "type": "sylius-plugin",
    "keywords": ["Netaxept", "gateway", "payment", "sylius", "sylius-plugin"],
    "description": "Integration of Netaxept API to Sylius as a new payment method.",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "sylius/sylius": "^1.2",
        "fdm/payum-netaxept": "^0.9.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Fronty\\SyliusNetaxeptPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/Kernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
